package javaexp.z01_home;

public class A12_0419 {

	public static void main(String[] args) {
		

	}

}

class Team {
	String name;
	int win;
	int same;
	int loose;
	int percent;
	public Team() {
		// TODO Auto-generated constructor stub
	}
	public Team(String name, int win, int same, int loose, int percent) {
		this(name);
		this.win = win;
		this.same = same;
		this.loose = loose;
		this.percent = percent;
	}
	public Team(String name, int win, int same, int loose) {
		this.name = name;
		this.win = win;
		this.same = same;
		this.loose = loose;
	}
	public Team(String name, int win, int same) {
		this.name = name;
		this.win = win;
		this.same = same;
	}
	public Team(String name, int win) {
		this.name = name;
		this.win = win;
	}
	public Team(String name) {
		this.name = name;
	}
	
	
	
}