package javaexp.z01_home;

public class A12_0419 {

	public static void main(String[] args) {
		Team t = new Team("one", 3, 3, 3, 3);
		Yellow y1 = new Yellow();

	}

}

class Team {
	String name;
	int win;
	int same;
	int loose;
	double percent;
	
	public Team(int win) {
		this.win = win;
		System.out.println("상위 객체 생성");
	}

	public Team(String name, int win, int same, int loose, int percent) {
		this.name = name;
		this.win = win;
		this.same = same;
		this.loose = loose;
		this.percent = percent;
	}



	public Team(String name) {
		this.name = name;
	}
	
	public void result() {
		System.out.println("--- Team 메서드 ---");
	}
}

class Yellow extends Team {

	public Yellow() {
		super(3);
	}
	
	public void result() {
		System.out.println("--- Yellow 메서드 ---");
	}
	
}