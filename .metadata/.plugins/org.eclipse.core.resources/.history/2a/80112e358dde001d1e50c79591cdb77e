package javaexp.z01_home;

public class A12_0419 {

	public static void main(String[] args) {
		

	}

}

class Team {
	String name;
	int win;
	int same;
	int loose;
	int percent;
	
	public Team(int win) {
		this.win = win;
		System.out.println();
	}
	public Team(String name, int win, int same, int loose, int percent) {
		this(name, win, same, loose);
		this.percent = percent;
	}
	public Team(String name, int win, int same, int loose) {
		this.name = name;
		this.win = win;
		this.same = same;
		this.loose = loose;
	}
	public Team(String name, int win, int same) {
		this.name = name;
		this.win = win;
		this.same = same;
	}
	public Team(String name, int win) {
		this.name = name;
		this.win = win;
	}
	public Team(String name) {
		this.name = name;
	}
	
	public void result() {
		System.out.println("--- Team 메서드 ---");
	}
}

class Yellow extends Team {
	
}