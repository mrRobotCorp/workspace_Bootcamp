package javaexp.a09_inherit;

public class A09_PolyExp {

	public static void main(String[] args) {
		/*
		 ex) Duck 상위
				종류
		 		flying() 재정의
		 NormalDuck
		 		집오리
				flying() 날지 못함
		Mallard
			청둥오리
			flying() 날아 다님
		상하위 구조로 다형성 처리하여 메서드 호출
*/

	}

}
class Duck {
	private String kind;
	
	public Duck(String kind) {
		this.kind = kind;
	}

	public void flying() {
		System.out.println(kind);
	}
}

class NormalDuck extends Duck {

	public NormalDuck(String kind) {
		super(kind);
	}
	
	public void flying() {
		
	}
	
}

class Mallard extends Duck {

	public Mallard(String kind) {
		super(kind);
	}
	
	public void flying() {
		
	}
	
}