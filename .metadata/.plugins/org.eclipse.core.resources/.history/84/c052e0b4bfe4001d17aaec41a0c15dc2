package javaexp.a11_api;

public class A02_String {

	public static void main(String[] args) {
		char [] arr = {'H', 'e', 'l', 'l', 'o'};
		String str = new String(arr);
		System.out.println("문자열 데이터 생성 : " + str);
		byte [] arr1 = new byte[8];
		
		for(int i = 0; i <= 255; i++) {
			System.out.println(i + "\t" + (char)i);
		}
	}

}
