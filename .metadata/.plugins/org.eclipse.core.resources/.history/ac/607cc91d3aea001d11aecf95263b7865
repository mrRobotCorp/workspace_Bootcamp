package javaexp.a13_io;

import java.io.File;
import java.io.IOException;

public class A09_NIO {

	public static void main(String[] args) {
		String fname = "E:\\a01_javaexp\\workspace_Bootcamp\\javaexp\\src\\javaexp\\a13_io\\";
		File f01 = new File(fname + "z05_data.txt");
		
		if(!f01.exists()) {
			try {
				f01.createNewFile();
			} catch (IOException e) {
				System.out.println(e.getMessage());
			}
		}
		
	}

}
