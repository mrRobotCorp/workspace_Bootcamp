	public static void main(String[] args) {
		

	}

}

class Team {
	String name;
	int win;
	int same;
	int loose;
	int percent;
	
	public Team(int win) {
		this.win = win;
		System.out.println("상위 객체 생성");
	}

	public Team(String name) {
		this.name = name;
	}
	
	public void result() {
		System.out.println("--- Team 메서드 ---");
	}
}

class Yellow extends Team {

	public Yellow() {
		super(3);
	}
	
	public void result() {
		System.out.println("--- Yellow 메서드 ---");
	}
	
}