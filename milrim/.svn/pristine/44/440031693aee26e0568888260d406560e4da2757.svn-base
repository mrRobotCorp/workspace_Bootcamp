body { background: #f7f7f7; } 

.sidebar { display: block; position: fixed; top: 60px; bottom: 0; left: 0; padding: 0; overflow-x: hidden; overflow-y: auto; box-shadow: none; background: #fff; z-index: 50; } 

.sidebar ul.nav li.active a, 
.sidebar ul.nav .active > a:hover, 
.sidebar ul.nav .active > a:focus { background-color: var(--color-sub); } 

.sidebar ul.nav { margin-top: 50px; } 
.sidebar ul.nav li { display: flex; align-items: center; justify-content: center; height: 50px; width: 230px; } 
.sidebar ul.nav li a { display: flex; justify-content: flex-start; align-items: center; width: 120px; height: 20px; padding: 10px 30px; font-size: 15px; border-radius: 20px; transition: all .2s; } 
.sidebar ul.nav a:hover { background-color: #eee; } 

.nav > li > a:hover, .nav > li > a:focus, .nav .open > a, .nav .open > a:hover, .nav .open > a:focus { background-color: rgba(255, 255, 255, 0.2); background: none; } 

.nav > li > a img { margin-right: 15px; width: 22px; } 
/* ------------------------------------------------------ */

.memSection,
.vidSection,
.pstSection { width: calc(100vw * 1300/ 1920); margin: 110px 0 0 22%; } 

.listBox { margin-top: 20px; } 
.listBox #allCh { display: none; } 
.listBox #allCh + label { padding: 6px 12px; margin: 0 10px; border-radius: 7px; font-size: 14px; background-color: #bde0ff; cursor: pointer; transition: all .2s; } 
.listBox #allCh + label:hover { background-color: #92cafc; } 

.uptBtn, .delBtn { padding: 6px 12px; border-radius: 7px; font-size: 14px; font-family: pMedium; background: transparent; transition: all .2s; cursor: pointer; } 
.uptBtn { margin-right: 10px; background-color: #ffe395; } 
.uptBtn:hover { background-color: #ffd562; } 
.delBtn { background-color: #ffc2b8; } 
.delBtn:hover { background-color: #ff9f91; } 

.schBox { width: 95%; padding: 0 20px; display: flex; align-items: center; justify-content: flex-start; height: 50px; border-radius: 30px; background: #e0e0e0; border: 2px solid transparent; transition: all .3s ease; } 
.schBox button { width: 20px; height: 20px; margin: 0 25px 0 12px; background: url(../source/searchIco.png) center center; background-size: cover; cursor: pointer; } 
.schBox input { width: 90%; background: transparent; font-size: 17px; font-family: pRegular; } 
.schBox:focus-within { background: #fff; border: 2px solid var(--color-main); } 

.listAll { width: 100%; margin-right: 100px; overflow: hidden;} 
.listAll li ul { display: flex; align-items: center; justify-content: space-around; } 

.listHead { display: flex; align-items: center; justify-content: space-around; margin-top: 20px; height: 50px; padding-left: 80px; background: url(../source/bgAdm.png) center center; background-size: cover; color: #fff; border-radius: 20px; font-family: pBold; } 
.listHead.vid { padding-left: 30px;}
.listHead li { text-shadow: 0 0 4px #33333374; font-size: 16px; text-align: center; } 

.listRow .memberInfo,
.pstInfo, .vidInfo { padding-left: 80px; width: 100%; height: 100%; display: flex; align-items: center; justify-content: space-around; } 
.vidInfo { padding-left: 30px;}

.memberInfo li,
.pstInfo li,
.vidInfo li { text-align: center; } 

.listHead.mem li,
.memberInfo li { width: 80px; } 

.listHead.mem li:nth-child(4), .listHead.mem li:nth-child(5),
.memberInfo li:nth-child(4), .memberInfo li:nth-child(5) { width: 170px; } 

.listHead.pst li:first-child,
.pstInfo li:first-child { width: 260px; } 

.listHead.pst li:nth-child(2),
.pstInfo li:nth-child(2) { width: 110px;}
.listHead.vid li:first-child { width: 310px; } 

.listHead.vid li:nth-child(2), .vidInfo li:nth-child(2),
.listHead.vid li:last-child, .vidInfo li:last-child { width: 110px; } 

.pstInfo li:nth-child(2) { width: 110px; } 

.listRow { position: relative; width: 100%; height: 60px; font-family: pLight; border-bottom: 1px solid #ddd; transition: all .25s; cursor: pointer; } 
.vidSection .listRow { height: 120px; } 
.listRow:hover { background-color: #e4e4e4; border-radius: 20px; } 
.listRow input[type="checkbox"] { display: none; } 
.listRow input[type="checkbox"] + label { position: absolute; width: 22px; height: 22px; top: 18px; left: 40px; background: url(../source/checkIco.png) center center; background-size: cover; transition: all .2s; cursor: pointer; opacity: .5; } 
.listRow input[type="checkbox"] + label:hover { opacity: 1; } 
.listRow input[type="checkbox"]:checked + label { background: url(../source/checkedIco.png) center center; background-size: cover; opacity: 1; } 

.vidCheck input[type="checkbox"] + label { top: 50px; left: 70px; } 

.vidInfo .vidTitle { width: 310px; display: flex; align-items: center; justify-content: center; text-align: left; } 
.vidInfo .vidTitle img { width: 140px; margin-right: 15px; border-radius: 10px; border: 1px solid #e6e6e6; } 
.vidInfo .vidTitle div p:first-child { font-size: 16px; font-family: pSemi; } 
.vidInfo .vidTitle div p:last-child { margin-top: 5px; color: #666; font-size: 14px; font-family: pLight; } 

.epShowBtn { position: absolute; display: flex; width: 27px; height: 27px; align-items: center; justify-content: center; right: 20px; padding: 5px; border-radius: 5px; background-color: transparent; cursor: pointer; transition: all .3s;} 
.epShowBtn:hover { background: #fff;}
.epShowBtn img {transition: all .3s; }
.epShowBtn.active img { transform: rotate(180deg); }

.epListAll::-webkit-scrollbar { height: 13px; } 
.epListAll::-webkit-scrollbar-thumb { background-color: #777; border: 3px solid #f7f7f7; border-radius: 20px; } 
.epListAll::-webkit-scrollbar-track { background-color: #f7f7f7; } 

.epListAll { display: none; align-items: center; padding: 10px 20px; height: 90px; border-bottom: 1px solid #ddd; background-color: #fff5da; overflow-x: auto; overflow-y: hidden;} 
.epListAll.active { display: flex; } 
.epList { display: flex; align-items: center; } 
.epList li { width:max-content; height: 45px; margin-right: 30px; padding: 17px 20px; box-shadow: 1px 1px 5px #c4c4c498; background-color: #ffffffc4; border-radius: 15px; cursor: pointer; transition: all .3s;} 
.epList li:hover { background: #ffffff; }
.epList li p:first-child { font-size: 13px; color: #777; } 
.epList li p { font-size: 14px; margin-bottom: 10px; } 
.epList li:last-child { text-align: center; } 
.epList .addBtn { width: 40px; margin-top: 5px; opacity: .6; transition: all .3s; } 
.epList .addBtn:hover { opacity: 1; } 

/* ------------------------------------------- */
.pagination { margin-top: 20px; display: flex; align-items: center; justify-content: center; } 
.pagination li { display: inline-block; margin: 0 3px; border-radius: 3px; text-align: center; vertical-align: top; height: 25px; transition: all .2s; } 
.pagination li:hover { background-color: #ddd; } 

.pagination li a { display: inline-block; font-size: 15px; padding: 0 8px; line-height: 25px; } 
.pagination li.active { background-color: var(--color-main); } 
.pagination li.active a { color: #fff; } 

.pagination li.pages ul li { display: inline-block; float: none; } 

/* ------------------- modal ---------------------- */
.modal { display: none; position: fixed; left: 0; right: 0; background-color: #fff; padding: 30px 20px; max-height: 70%; width: 450px; margin: auto; overflow-y: auto; border-radius: 10px; will-change: top, opacity; box-shadow: 1px 1px 18px #b8b8b874; } 
.modal h4 { margin-bottom: 25px; font-size: 20px; font-family: pBold; text-align: center; } 

.modalCon { position: relative; }
.modalCon .modal-close { position: absolute; width: 17px; height: 17px; top: 0; right: 8px; background: url(../source/closeIco.png) center center; background-size: cover; cursor: pointer; }

.modal .uptRow { display: flex; align-items: center; justify-content: center; padding-bottom: 13px; margin: 0 10px 10px 10px; border-bottom: 1px solid #e1e1e1; } 
.modal .uptRow .uptSub { margin-right: 15px; width: 70px; font-size: 15px; font-family: pSemi; } 
.modal .uptRow input { padding: 10px 15px; width: 230px; font-family: pRegular; background-color: #f0f0f0; transition: all .2s; } 
.modal .uptRow input:focus { background-color: #e1f1ff; } 

.modal .modalUpt { float: right; padding: 6px 12px; margin: 10px 10px 0 0; border-radius: 7px; font-size: 14px; font-family: pMedium; background: #ffe395; transition: all .2s; cursor: pointer; } 
.modal .modalUpt:hover { background-color: #ffd562; } 
.modal .modalUpt:active { background: #c2c2c2; } 

.darkBg { display: none; position: fixed; top: 0; left: 0; width: 100vw; height: 100vh; background: #00000031; z-index: 10; } 
.darkBg.active { display: block; } 